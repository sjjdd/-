.card {
  margin-top: 100px;
  border: 2px solid #ffffff;
  border-radius: 5px;
  background-color: #ffffff;
  box-shadow: 4px 1px 1px #cccccc;
  margin: 8px;
  position: relative;
}

.image {
  width: 100%;
  height: 200px;
  background-color: #eeeeee;
}

.time {
  color:#000;
  position: relative;
  top: 0;
  z-index: 1;
  font-size: 15px;
}
.tip{
  widht:100%;
