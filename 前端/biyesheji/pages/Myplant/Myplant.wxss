  background-color: #43c729;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.tip view{
  margin:0 auto;
  font-size:13px;
  color:#fff;
  font-style:italic;
  font-family: Arial;
  text-align: center;
}
